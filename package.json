{
  "name": "sequelize-boilerplate",
  "version": "1.0.0",
  "description": "nodejs boilerplate",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "start:prod": "NODE_ENVIRONMENT=prod ./node_modules/.bin/sequelize db:migrate && NODE_ENVIRONMENT=prod nodemon server.js",
    "start:stag": "NODE_ENVIRONMENT=stag nodemon server.js",
    "createDb:stag": "NODE_ENVIRONMENT=stag node dbCreate.js",
    "createDb:prod": "NODE_ENVIRONMENT=prod node dbCreate.js",
    "dropDb:stag": "NODE_ENVIRONMENT=stag node dropDb.js",
    "dropDb:prod": "NODE_ENVIRONMENT=prod node dropDb.js",
    "migration:stag": "NODE_ENVIRONMENT=stag ./node_modules/.bin/sequelize db:migrate",
    "migration:prod": "NODE_ENVIRONMENT=prod ./node_modules/.bin/sequelize db:migrate",
    "migration:undo:stag": "NODE_ENVIRONMENT=stag ./node_modules/.bin/sequelize db:migrate:undo:all",
    "migration:undo:prod": "NODE_ENVIRONMENT=prod ./node_modules/.bin/sequelize db:migrate:undo:all",
    "seed:stag": "NODE_ENVIRONMENT=stag ./node_modules/.bin/sequelize db:seed:all",
    "seed:prod": "NODE_ENVIRONMENT=prod ./node_modules/.bin/sequelize db:seed:all",
    "seed:undo:stag": "NODE_ENVIRONMENT=stag ./node_modules/.bin/sequelize db:seed:undo",
    "seed:undo:prod": "NODE_ENVIRONMENT=prod ./node_modules/.bin/sequelize db:seed:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "axios": "^0.19.0",
    "body-parser": "^1.20.1",
    "core-js": "^3.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-validation": "^1.0.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.2",
    "pg": "^7.15.1",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1"
  }
}
